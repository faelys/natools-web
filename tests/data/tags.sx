(root tags)
(tag-elements
   (title "Tag " (current (name (2))))
   (main-text "<p>Pages:</p>\n"
      (current "<ul>\n"
         (all-elements (render "<li>" (element title) "</li>\n"))
         "</ul>\n"))
   (sidebar
      "<ul>\n"
      (current (lesser-children (render "<li>" (name (2)) "</li>\n")))
      "<li style=\"current\">" (current (name (2))) "</li>\n"
      (current (greater-children (render "<li>" (name (2)) "</li>\n")))
      "<ul>\n"))
(index-elements
   (title "All tags")
   (main-text
      (current
         (all-children
            (render
               "<h2>" (name (2)) "</h2>\n<ul>\n"
               (all-elements (render "<li>" (element title) "<li>\n"))
               "</ul>\n"))))
   (sidebar
      (current
         "<ul>\n"
         (all-leaves (render "<li>" (name (2 last)) "</li>\n"))
         "</ul>\n")))
